pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/panzo11/jenkins_teste.git'
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
                sh 'npm run build'
            }
        }
        stage('Deploy') {
            steps {
                sh 'scp -r dist/* files.000webhost.com
'
            }
        }
    }
}
